defmodule CrossEntropy do
  import Nx.Defn

  defn binary_cross_entropy(y_true, y_pred) do
    y_true * Nx.log(y_pred) - (1 - y_true) * Nx.log(1 - y_pred)
  end
end

for x <- [0.455, 0.333, 0.999, 0.8], do:
